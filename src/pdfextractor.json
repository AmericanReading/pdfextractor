{
    // Add blank pages at the specified indexed.
    // Override with --blank
    // "blank": [2, 4]
    "blank": [],

    // Box model to use for determining PDF page size. Allowed values: "media", "crop", "trim"
    "box": "trim",

    // Convert to a given colorspace.
    // Override with --colorspace
    // For a list of options, see http://www.imagemagick.org/script/command-line-options.php#colorspace
    // or run "identify -list colorspace"
    // "colorspace": "RGB"
    "colorspace": null,

    // Source density for ImageMagick commands.
    // Override with --density
    "density": null,

    // Resize dimensions as string.
    // Override with --resize
    // "resize": "1920x1536"
    "resize": null,

    // Start numbering pages from this number.
    // Override with --start
    "start": 1,

    // Output directory. Override with --target or -o
    "page-pattern": "%03d.jpg",

    // Number of child process to run simultaneously.
    // Override with --concurrent
    "concurrent": 8,

    // Paths to ImageMagick commands.
    "convert": "convert",
    "identify": "identify"
}
